<%- include('partials/header'); %>

<h1>Compose</h1>
<form class="" action="/compose" method="post">

  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" required autocomplete="off">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="8" cols="80" required></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="publish">Publish</button>

</form>

<%- include('partials/footer'); %>